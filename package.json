{
  "name": "react-inline-loaders",
  "version": "1.0.6",
  "description": "React inline loaders (components)",
  "keywords": [
    "loaders",
    "react",
    "reactjs"
  ],
  "homepage": "https://github.com/raulanatol/react-inline-loaders#readme",
  "bugs": {
    "url": "https://github.com/raulanatol/react-inline-loaders/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raulanatol/react-inline-loaders.git"
  },
  "license": "MIT",
  "author": "raulanatol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "analyze": "source-map-explorer lib/index.js",
    "analyze-bundle": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "build-storybook": "rm -rf gh-pages && build-storybook -o gh-pages",
    "dist": "npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc",
    "lint": "eslint --max-warnings 0 . --ext .ts,.tsx",
    "clean": "shx rm -rf dist build",
    "postpublish": "yarn run build-storybook && gh-pages -d gh-pages",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/after.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@cowcoders/eslint-config": "^1.5.5",
    "@emotion/styled": "^11.8.1",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/addons": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/storybook-deployer": "^2.8.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/jest": "^27.5.0",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "autoprefixer": "^10.4.7",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "babel-runtime": "^6.26.0",
    "caniuse-lite": "^1.0.30001335",
    "cssnano": "^5.1.7",
    "dts-bundle": "^0.7.3",
    "eslint": "^8.14.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "gh-pages": "^3.2.3",
    "github-release-notes": "^0.17.3",
    "jest": "27.5.1",
    "jest-css-modules": "^2.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-markdown": "^8.0.3",
    "shx": "^0.3.4",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "peerDependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.8.1"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "engines": {
    "node": "=16.15.0",
    "npm": "=8.5.5"
  },
  "engineStrict": true,
  "dependencies": {
    "@emotion/react": "^11.9.0"
  }
}
